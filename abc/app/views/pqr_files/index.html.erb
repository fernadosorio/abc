<p id="notice"><%= notice %></p>

<h1>Pqr Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pqr_files.each do |pqr_file| %>
      <tr>
        <td><%= link_to 'Show', pqr_file %></td>
        <td><%= link_to 'Edit', edit_pqr_file_path(pqr_file) %></td>
        <td><%= link_to 'Destroy', pqr_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pqr File', new_pqr_file_path %>
